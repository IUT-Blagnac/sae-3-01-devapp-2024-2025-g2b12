ALTER TABLE `APPARENTER`
  ADD PRIMARY KEY (`idVariete1`,`idVariete2`),
  ADD KEY `fk_idVariete2_apparenter_variete` (`idVariete2`);

--
-- Index pour la table `AVIS`
--
ALTER TABLE `AVIS`
  ADD PRIMARY KEY (`idClient`,`idProduit`),
  ADD KEY `fk_idProduit_avis_produit` (`idProduit`),
  ADD KEY `fk_idClient_avis_client` (`idClient`);

--
-- Index pour la table `CARTE_BANCAIRE`
--
ALTER TABLE `CARTE_BANCAIRE`
  ADD PRIMARY KEY (`numCB`);

