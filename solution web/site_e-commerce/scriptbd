-- Index pour la table `COMMANDE`
--
ALTER TABLE `COMMANDE`
  ADD PRIMARY KEY (`idCommande`),
  ADD KEY `fk_idClient_commande_client` (`idClient`),
  ADD KEY `fk_idAdresse_commande_adresse` (`idAdresse`),
  ADD KEY `fk_idRelais_commande_relais` (`idRelais`),
  ADD KEY `fk_numCB_commande_carte_bancaire` (`numCB`);

--
-- Index pour la table `COMMANDER`
--
ALTER TABLE `COMMANDER`
  ADD PRIMARY KEY (`idCommande`,`idVariete`),
  ADD KEY `fk_idVariete_commander_variete` (`idVariete`);

